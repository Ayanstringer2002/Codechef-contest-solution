#include <iostream>
#include <string>
#include <unordered_set>

using namespace std;

int maxLength(string s) {
    int n = s.length();
    int i = 0, j = 0, ans = 0;
    unordered_set<char> st;
    while (i < n && j < n) {
        if (st.find(s[j]) == st.end()) {
            st.insert(s[j++]);
            ans = max(ans, j - i);
        } else {
            st.erase(s[i++]);
        }
    }
    return ans;
}

int main() {
    int t;
    cin >> t;
    while (t--) {
        string s;
        cin >> s;
        cout << maxLength(s) << endl;
    }
    return 0;
}
